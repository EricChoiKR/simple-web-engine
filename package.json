    "url": "https://github.com/EricChoiKR/simple-web-engine/issues"
  },
  "homepage": "https://github.com/EricChoiKR/simple-web-engine#readme",
  "dependencies": {
    "@types/express": "^4.16.0",
    "config": "^2.0.1",
    "express": "^4.16.4"
  },
  "devDependencies": {
    "nodemon": "^1.18.4",
    "ts-node": "^7.0.1",
    "tsc": "^1.20150623.0",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3"
  }
